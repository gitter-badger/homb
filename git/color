#!/bin/bash
################################################################################
# Git Colors
#
# File that has the different colors used by git
#
################################################################################

export GIT_PS1_COLOR="${BBlack}" # All clear
export GIT_PS1_COLOR_UNSTAGED="${BYellow}" # "*" Modifications are not committed
export GIT_PS1_COLOR_STAGED="${Green}" # "+" Added files to stage uncommitted
export GIT_PS1_COLOR_STASHED="${Cyan}" # "$" If somethings is stashed.
export GIT_PS1_COLOR_UNTRACKED="${Red}" # "%" Files are not being tracked 
